		"addressPrefixes": {
			"value": "10.0.0.0/14"
		},
		"masterSubnetName": {
			"value": "mastersubnet"
		},
		"masterSubnetPrefix": {
			"value": "10.1.0.0/16"
		},
		"infraSubnetName": {
			"value": "infrasubnet"
		},
		"infraSubnetPrefix": {
			"value": "10.2.0.0/16"
		},
		"nodeSubnetName": {
			"value": "nodesubnet"
		},
		"nodeSubnetPrefix": {
			"value": "10.3.0.0/16"
		},
		"masterClusterType": {
			"value": "public"
		},
		"routerClusterType": {
			"value": "public"
		},
		"routingCertType": {
			"value": "selfsigned"
		},
		"masterCertType": {
			"value": "selfsigned"
		}
	}
}
