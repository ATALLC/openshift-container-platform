{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"_artifactsLocation": {
			"value": "https://raw.githubusercontent.com/ATALLC/openshift-container-platform/master"
		},
		"masterVmSize": {
			"value": "Standard_E2s_v3"
		},
		"infraVmSize": {
			"value": "Standard_D4s_v3"
		},
		"nodeVmSize": {
			"value": "Standard_D4s_v3"
		},
		"cnsVmSize": {
			"value": "Standard_E4s_v3"
		},
		"osImageType": {
			"value": "marketplace"
		},
		"marketplaceOsImage": {
			"value": {
				"publisher": "IMGPUBLISHER",
                "offer": "IMGOFFER",
                "sku": "IMGSKU",
                "version": "IMGVERSION"
			}
		},
		"storageKind": {
			"value": "managed"
		},
		"openshiftClusterPrefix": {
			"value": "ocpcluster"
		},
		"minorVersion": {
			"value": "69"
		},
		"masterInstanceCount": {
			"value": MASTERCOUNT
		},
		"infraInstanceCount": {
			"value": INFRCOUNT
		},
		"nodeInstanceCount": {
			"value": NODECOUNT
		},
		"cnsInstanceCount": {
			"value": 3
		},
		"dataDiskSize": {
			"value": 128
		},
		"cnsGlusterDiskSize": {
			"value": 128
		},
		"adminUsername": {
			"value": "OSADMINUSERNAME"
		},
		"enableMetrics": {
			"value": "true"
		},
		"enableLogging": {
			"value": "true"
		},
		"enableCNS": {
			"value": "false"
		},
		"rhsmUsernameOrOrgId": {
			"value": "RHSMORGID"
		},
		"rhsmPoolId": {
			"value": "RHSMPOOLID"
		},
		"rhsmBrokerPoolId": {
			"value": "RHSMPOOLID"
		},
		"sshPublicKey": {
			"value": "SSHPUBLICKEY"
		},
		"keyVaultSubscriptionId": {
			"value": "KEYVAULTSUBSCRIPTIONID"
		},
		"keyVaultResourceGroup": {
			"value": "KEYVAULTRESOURCEGROUP"
		},
		"keyVaultName": {
			"value": "KEYVAULTNAME"
		},
		"enableAzure": {
			"value": "true"
		},
		"aadClientId": {
			"value": "AADCLIENTID"
